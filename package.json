        "doc": "docs",
        "lib": "lib",
        "test": "test"
    },
    "scripts": {
        "test": "ts-node ./test/test.ts",
        "build-bowser": "browserify --standalone excelconvert ./excelconvert/excelconvert.js > ./dist/excelconvert.js",
